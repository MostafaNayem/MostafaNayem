<script>
// Alert Redirect to Another Link
 $(document).on('click', '#link50', function(e) {
 swal({
 title: "Are you sure?", 
 text: "You will be redirected to payment center", 
 type: "warning",
 confirmButtonText: "Confirm",
 showCancelButton: true
 })
 .then((result) => {
 if (result.value) {
 window.location = 'en-topup-50.html';
 } else if (result.dismiss === 'cancel') {
 swal(
 'Cancelled',
 'Your stay here :)',
 'error'
 )
 }
 })
 });
 </script>

